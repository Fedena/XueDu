<div id="content-header">
    <img src="/images/show_settings.png" alt="Schools" />
  <h1>Compuses</h1>
  <h3>Home</h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">

  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

  <div class="box">

    <div class="link-box">
      <div class="link-heading"><%= link_to 'Manage compuses' ,:action=>'manage_compus' %> </div>
      <div class="link-descr">Manage Compus and create classrooms</div>
    </div>
    <div class="link-box">
      <div class="link-heading"><%= link_to 'Manage Classrooms' ,:action=>'manage_classrooms' %>  </div>
      <div class="link-descr">Manage active classrooms</div>
    </div>

    <div class="extender"></div>
  </div>
</div>
